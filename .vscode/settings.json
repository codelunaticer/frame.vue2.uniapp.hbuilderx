{
  "javascript.suggest.paths": true,
  "path-autocomplete.extensionOnImport": true,
  "files.associations": {
    "pages.json": "jsonc",
    "manifest.json": "jsonc"
  },
  "path-autocomplete.pathMappings": {
    "@": "${folder}/",
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/uni_modules": true
  },
  "cssVariables.lookupFiles": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "../static/styles/variables.css"
  ]
}